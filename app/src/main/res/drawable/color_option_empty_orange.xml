<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>

        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/event_darker_orange" />
            <corners android:radius="20dp" />
            <size
                android:width="12dp"
                android:height="12dp" />
            <padding
                android:bottom="4dp"
                android:left="4dp"
                android:right="4dp"
                android:top="4dp" />
        </shape>

    </item>
    <item>

        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/white" />
            <corners android:radius="20dp" />
            <size
                android:width="12dp"
                android:height="12dp" />

        </shape>

    </item>


</layer-list>